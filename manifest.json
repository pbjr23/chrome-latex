{
  "name": "ChromeTeX",
  "version": "0.0.2",
  "background": {
    "scripts": ["background.js"]
  },
  "description": "View LaTeX files (.tex) directly in Chrome!",
  "manifest_version": 2,
  "permissions": ["webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],
  "content_scripts": [
    {
        "matches": [
            "*://*/*.tex"
        ],
        "js": [
            "content.js"
        ],
        "run_at": "document_end"
    }
    ]
}
